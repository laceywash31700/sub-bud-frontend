{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@babel/*": ["node_modules/@babel/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "types": ["vite/client"],
    "typeRoots": [
      "./node_modules",
      "./node_modules/@types",
      "./node_modules/vite",
      "./src/utils",
      "./src/__Test__/Test.utils"
    ],
    "jsx": "react-jsx"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.d.ts",
    "vite-env.d.ts"
  ]
}